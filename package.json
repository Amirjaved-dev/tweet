{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:quiet": "cross-env NODE_ENV=development LOG_LEVEL=ERROR tsx server/index.ts",
    "dev:minimal": "cross-env NODE_ENV=development LOG_LEVEL=WARN tsx server/index.ts",
    "dev:verbose": "cross-env NODE_ENV=development LOG_LEVEL=DEBUG tsx server/index.ts",
    "dev:no-warnings": "cross-env NODE_ENV=development tsx server/index.ts 2>&1 | grep -v \"Sourcemap for.*points to missing source files\"",
    "build": "vite build && tsc -p tsconfig.server.json && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:prod": "cross-env NODE_ENV=production vite build && tsc -p tsconfig.server.json && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --minify",
    "build:vercel": "NODE_ENV=production VERCEL=1 vite build && tsc -p tsconfig.server.json && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --minify",
    "postinstall": "npm i -g cross-env esbuild typescript vite",
    "vercel-build": "node vercel.build.js",
    "start": "NODE_ENV=production node dist/index.js",
    "start:prod": "pm2 start ecosystem.config.js --env production",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:payment-setup": "psql -f payment-tables.sql",
    "db:backup": "pg_dump -U postgres threadflowpro > backups/backup-$(date +%Y%m%d%H%M%S).sql",
    "db:restore": "psql -U postgres threadflowpro < ",
    "test-server": "cross-env NODE_ENV=development tsx server/scripts/test-server.ts",
    "payment:test": "node scripts/test-payment-system.js",
    "payment:test-health": "node scripts/test-payment-system.js health",
    "payment:test-plans": "node scripts/test-payment-system.js plans",
    "payment:test-charge": "node scripts/test-payment-system.js charge",
    "payment:test-webhook": "node scripts/test-payment-system.js endpoint",
    "payment:test-full": "node scripts/test-payment-system.js full",
    "payment:simulate-webhook": "node scripts/test-payment-system.js webhook",
    "docker:build": "docker build -t threadflowpro:latest .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.production threadflowpro:latest",
    "docker:compose": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "deploy": "pm2 deploy ecosystem.config.js production",
    "logs": "pm2 logs threadflowpro",
    "monitor": "pm2 monit",
    "resize-logo": "node scripts/resize-logo.js"
  },
